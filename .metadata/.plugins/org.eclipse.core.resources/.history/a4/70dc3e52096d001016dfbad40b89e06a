package spring.mvc.basic_ict05.ch10;

import org.springframework.stereotype.Component;

@Component("sony")
public class SonySpeaker {
	
	// volUp volDown
	public void volUp() {
		System.out.println("SonySpeaker - volUp()");
	}
	
	public void volDown() {
		System.out.println("SonySpeaker - volDown()");
	}
}
